<div class="page-header">

</div>

<div class= "well form-horizontal col-md-4 col-sm-offset-4">
<h2>Sign Up for gCamp!</h2>
<br>

<%= form_tag signin_path do |f| %>

  <% if @sign_in_error %>
    <p>
      <%= @sign_in_error %>
    </p>
  <% end %>






  <div>
    <%= label_tag :first_name, "First Name" %>
    </div>
    <div>
    <%= text_field_tag :first_name, nil, class:"form-control" %>
  </div>


  <div>
    <%= label_tag :last_name, "Last Name" %>
    </div>
    <div>
      <%= text_field_tag :last_name, nil, class:"form-control" %>
  </div>




<div>
    <%= label_tag :email %>
  </div>
  <div>
    <%= email_field_tag :email, nil, class:"form-control" %>
  </div>

  <div>
    <%= label_tag :password %>
  </div>
  <div>
    <%= password_field_tag :password, nil, class:"form-control" %>
  </div>

<br>

  <div>
    <%= submit_tag "Sign in", class:"btn btn-primary" %>
  </div>



<% end %>


</div>
